        "# Enter your code here. Read input from STDIN. Print output to STDOUT\n",
        "\n",
        "p = 1.0 / 3.0 \n",
        "n = 6\n",
        "geodist = 0\n",
        "\n",
        "for i in range(1, 6):\n",
        "    geodist += (1-p) ** (i-1) * p\n",
        "\n",
        "print(round(geodist, 3))"
