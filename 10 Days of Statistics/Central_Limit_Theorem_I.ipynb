      "source": [
        "import statistics\n",
        "import math\n",
        "\n",
        "upper_lim = int(input())\n",
        "n = int(input())\n",
        "mean = int(input())\n",
        "std = int(input())\n",
        "\n",
        "mean_prime = n * mean\n",
        "std_prime = math.sqrt(n) * std\n",
        "\n",
        "proba = 0.5 * (1 + math.erf((upper_lim - mean_prime) / (std_prime * math.sqrt(2))))\n",
        "\n",
        "print(round(proba, 4))\n"
      ]
    }
  ]
